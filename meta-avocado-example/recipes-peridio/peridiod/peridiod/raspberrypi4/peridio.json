{
  "version": 1,
  "device_api": {
    "certificate_path": "/etc/peridiod/peridio-cert.pem",
    "url": "device.cremini.peridio.com",
    "verify": true
  },
  "fwup": {
    "devpath": "/dev/mmcblk0",
    "public_keys": []
  },
  "remote_shell": true,
  "node": {
    "key_pair_source": "uboot-env",
    "key_pair_config": {
      "private_key": "peridio_key",
      "certificate": "peridio_certificate"
    }
  }
}
